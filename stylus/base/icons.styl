$localIcons = 'fonts/MaterialIcons/materialicons.woff2'
$nameIcons = 'Material Icons'
@font-face
	font-family: $nameIcons
	font-style: normal
	font-weight: 400
	src: url($localIcons) format('woff2')


.material-icons
	font-family: $nameIcons
	font-weight: normal
	font-style: normal
	font-size: 24px
	line-height: 1
	letter-spacing: normal
	text-transform: none
	display: inline-block
	word-wrap: normal
	-moz-font-feature-settings: 'liga'
	-moz-osx-font-smoothing: grayscale
